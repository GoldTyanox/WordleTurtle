                a.penup()
                a.pencolor("black")
                a.goto(-580+ ((square-1)*90) + 20,205 - ((row-1)*90) -60)
                a.pendown()
                a.write(i, align="center",font=("Clear Sans",50, "bold"))
                square = square + 1
    s.onkeypress(qP,"q")
    def rP():
        global selected
        global row
        global square
        global done
        global remove
        global selectList
        global word2
        remove = False
        letter = 0
        if len(selectList) <= (5 * row):
            if row <= 6:
                letter = letter + 1
                selectList.append("R")
            if len(selectList) > 5:
                selectList.pop(-1)
            a.clear()
            square = 1
            i = 0
            for i in selectList:
                a.penup()
                a.goto(-600 +((square - 1)*90), 150 - ((row-1)*90))
                a.pendown()
                a.pencolor("gray")
                a.pensize(5)
                for d in range(4):
                    a.forward(80)
                    a.left(90)
                a.penup()

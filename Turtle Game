    def enterP():
        v.clear()
        global selected
        global row
        global square
        global done
        global remove
        global correctL
        global selectList
        global word2
        remove = False
        letter = 0
        if len(selectList) <= (5 * row):
            if row <= 6:
                if len(selectList) == 5:
                    check("".join(selectList))
                    if valid == True:
                        if row <= 6:
                            square = 1
                            i=0
                            for i in range(len(selectList)):
                                if selectList[i] == word[i]:
                                    word2.append(selectList[i])
                                    if selectList[i] not in correct:
                                        correct.append(selectList[i])
                                        if selectList[i] in firstRow:
                                            t.setheading(0)
                                            move(0 + (60 * firstRow.index(selectList[i])),100)
                                            squareSequence(50, 80,selectList[i], True, "green")
                                            t.pencolor("white")
                                            t.penup()
                                            t.backward(35)
                                            t.left(90)
                                            t.forward(20)
                                            t.right(90)
                                            t.penup()
                                            t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                            t.pencolor("black")
                                        if selectList[i] in secondRow:
                                            t.setheading(0)
                                            move(30 + (60 * secondRow.index(selectList[i])),10)
                                            squareSequence(50, 80,selectList[i], True, "green")
                                            t.pencolor("white")
                                            t.penup()
                                            t.backward(35)
                                            t.left(90)
                                            t.forward(20)
                                            t.right(90)
                                            t.penup()
                                            t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                            t.pencolor("black")
                                        if selectList[i] in thirdRow:
                                            t.setheading(0)
                                            move(90 + (60 * thirdRow.index(selectList[i])),-80)
                                            squareSequence(50, 80,selectList[i], True, "green")
                                            t.pencolor("white")
                                            t.penup()
                                            t.backward(35)
                                            t.left(90)
                                            t.forward(20)
                                            t.right(90)
                                            t.penup()
                                            t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                            t.pencolor("black")

                            i = 0

                            for i in range(len(selectList)):
                                t.penup()
                                t.setheading(0)
                                t.goto(-600+ ((square-1)*90),150 - ((row-1)*90))


                                if selectList[i] == word[i]:
                                    
                                    squareSequence(80,80,"",True, "green")
                                    correctL = correctL + 1


                                elif selectList[i] in word:
                                    if word2.count(selectList[i]) < word.count(selectList[i]):
                                        squareSequence(80,80,"",True, "yellow")
                                        word2.append(selectList[i])
                                    else:
                                        squareSequence(80,80,"",True, "gray")
                                    if selectList[i] not in correct:
                                        if selectList[i] not in incorrect:
                                            incorrect.append(selectList[i])
                                            if selectList[i] in firstRow:
                                                t.setheading(0)
                                                move(0 + (60 * firstRow.index(selectList[i])),100)
                                                squareSequence(50, 80,selectList[i], True, "yellow")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                            if selectList[i] in secondRow:
                                                t.setheading(0)
                                                move(30 + (60 * secondRow.index(selectList[i])),10)
                                                squareSequence(50, 80,selectList[i], True, "yellow")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                            if selectList[i] in thirdRow:
                                                t.setheading(0)
                                                move(90 + (60 * thirdRow.index(selectList[i])),-80)
                                                squareSequence(50, 80,selectList[i], True, "yellow")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                else:
                                    squareSequence(80,80,"",True, "gray")
                                    if selectList[i] not in correct:
                                        if selectList[i] not in incorrect:
                                            if selectList[i] in firstRow:
                                                t.setheading(0)
                                                move(0 + (60 * firstRow.index(selectList[i])),100)
                                                squareSequence(50, 80,selectList[i], True, "gray")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                            if selectList[i] in secondRow:
                                                t.setheading(0)
                                                move(30 + (60 * secondRow.index(selectList[i])),10)
                                                squareSequence(50, 80,selectList[i], True, "gray")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                            if selectList[i] in thirdRow:
                                                t.setheading(0)
                                                move(90 + (60 * thirdRow.index(selectList[i])),-80)
                                                squareSequence(50, 80,selectList[i], True, "gray")
                                                t.pencolor("white")
                                                t.penup()
                                                t.backward(35)
                                                t.left(90)
                                                t.forward(20)
                                                t.right(90)
                                                t.penup()
                                                t.write(selectList[i],align="center",font=("Clear Sans",25,"bold"))
                                                t.pencolor("black")
                                t.pendown()
                                t.penup()
                                t.goto(-580+ ((square-1)*90) + 20,205 - ((row-1)*90) -60)
                                t.pendown()
                                t.pencolor("white")
                                t.write(selectList[i], align="center",font=("Clear Sans",50, "bold"))
                                t.pencolor("black")
            
                                square = square + 1
                            word2 = []
                            selectList.clear()
                            row = row +1
                            if correctL == 5:
                                v.penup()
                                v.goto(0,0)
                                v.pendown()
                                
                                l.color("white")
                                l.dot(10000)
                                l.color("black")
                                aword = "".join(word)
                                v.write('you win C: word: {} \n Press "TAB" to restart or click anywhere'.format(aword),align='center',font=('Clear Sans',30,"bold"))
                                s.onclick(clickrestart)
                                row = 7
                            else:
                                if row == 7:
                                    v.penup()
                                    v.goto(0,0)
                                    v.pendown()
                                    l.color("white")
                                    l.dot(10000)
                                    l.color("black")
                                    aword = "".join(word)
                                    v.write('you lose D: word: {} \n Press "TAB" to restart or click anywhere'.format(aword),align='center',font=('Clear Sans',30,"bold"))
                                    s.onclick(clickrestart)
                            correctL = 0
                    else:
                        v.penup()
                        v.goto(250,-200)
                        v.write("{} {}".format(dialogue1[random.randint(0,len(dialogue1)-1)],faces[random.randint(0,len(faces)-1)]),align= 'center',font = ('Clear Sans',30,'bold'))
                else:
                    v.penup()
                    v.goto(250,-200)

                    v.write("{} {}".format(dialogue2[random.randint(0,len(dialogue2)-1)],faces[random.randint(0,len(faces)-1)]),align='center',font = ('Clear Sans',30,'bold'))
                if len(selectList) > 5:
                    selectList.pop(-1)
                a.clear()
                square = 1
                i = 0
                for i in selectList:
                    a.penup()
                    a.goto(-600 +((square - 1)*90), 150 - ((row-1)*90))
                    a.pendown()
                    a.pencolor("gray")
                    a.pensize(5)
                    for d in range(4):
                        a.forward(80)
                        a.left(90)
                    a.penup()
                    a.pencolor("black")
                    a.goto(-580+ ((square-1)*90) + 20,205 - ((row-1)*90) -60)
                    a.pendown()
                    a.write(i, align="center",font=("Clear Sans",50, "bold"))
                    square = square + 1

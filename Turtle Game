    def move(x,y):
        t.penup()
        t.goto(x,y)
        t.pendown()
    def squareSequence(x,y, string, fill, color):
        t.pencolor("black")
        if fill == True:
            t.fillcolor(color)
            t.begin_fill()
        for i in range(2):
            t.pencolor("light gray")
            if fill == True:
                t.pencolor("light gray")
            t.forward(x)
            t.left(90)
            t.forward(y)
            t.left(90)
        if fill == True:
            t.end_fill()
            t.pencolor("black")
        t.penup()
        t.forward(25)
        t.left(90)
        t.forward(20)
        t.pendown()
        t.write(string, align="center", font=("Clear Sans", 25, "bold"))
        t.penup()
        t.backward(20)
        t.right(90)
        t.backward(25)
        t.forward(60)
        t.pendown()

    done = False
    move(-600, -300)
    t.pencolor("light gray")
